# Reload config
super + shift + r
 bspc wm -r

# Move desktops/windows
super + {_, shift} + {h,semicolon,1-9}
  bspc {desktop -f, node -d} {prev,next,1-9}

# Focus/swap with another window
super + {_, shift} + {j,k,i,l}
  bspc node -{f,s} {west,south,north,east}

# Toggle float/fullscreen
super + {_, shift} + f
  bspc node -t {'~fullscreen', '~floating'}

# Close window
super + d
  bspc node -c

